    "pify": "^2.3.0",
    "ramda": "^0.23.0",
    "source-map-support": "^0.4.11",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.22.1",
